   "source": [
    "# Combine the lists so that the result is a single list\n",
    "# with no duplciate values\n",
    "\n",
    "list1 = [\"a\", \"b\", \"c\"]\n",
    "list2 = [\"a\", \"e\", \"i\", \"o\", \"u\"]"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 3,
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "['a', 'b', 'c', 'i', 'u', 'o', 'e']\n"
     ]
    }
   ],
   "source": [
    "set1 = set(list1)\n",
    "set2 = set(list2)\n",
    "\n",
    "remove_dups = list(set2 - set1)\n",
    "combined_list = list1 + remove_dups\n",
    "\n",
    "print(combined_list)"
   ]
  }
 ],
